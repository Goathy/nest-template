{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@config/app/(.*)": "<rootDir>/../src/config/app/$1",
    "@config/app": "<rootDir>/../src/config/app",
    "@common/validations/(.*)": "<rootDir>/../src/common/validations/$1",
    "@common/validations": "<rootDir>/../src/common/validations",
    "@common/helpers/(.*)": "<rootDir>/../src/common/helpers/$1",
    "@common/helpers": "<rootDir>/../src/common/helpers"
  }
}
